<script>
function epochToDate(epochSecs) { 
    var m_names = new Array('Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'); 
    var d = new Date(new Date(0).setUTCSeconds(parseInt(epochSecs))); 
    return (m_names[d.getMonth()]+ " " + d.getDate()+ " " + d.getFullYear()); 
}
</script>
<style>
      /* 
  Resets defualt browser settings
  reset.css
*/

.news-right {
  float: none;
  width: 100%;
  line-height: 17px;
  border-bottom: 1px dashed #666;
}

#frame {
  position:absolute;
  z-index:0;
  width:739px;
  height:341px;
  top:-3px;
  left:-80px;
}
.date_field{
  margin-left: 10px;
font-style: italic;
font-size: 12px;
font-weight: normal;
}
.module {
  position: relative;
  margin: 0 0 10px;
  background: #f8f8f8;
  border: 1px solid #cccccc;
  padding: 10px;
}
.news_title {
  font-size: 20px;
  color: #000a11;
  font-weight: bold;
  padding: 6px 10px;
  background: #ddd;
}
.news_body{
  text-align: justify;
  line-height: 19px;
}
.news_body p{
  margin: 10px 0px;
}

</style>
</head>
<body>
{% include "header.dtl" %}
{% include "navigation.dtl" %}
<div class="container">
<div class="ad1 margin20_T"></div>
  <!-- ad1 end  -->

  <!-- bannerslides end -->
  <section class="contentarea margin20_T">
    <aside class="leftcontent">
      <div class="module">
        <div class="slideshow">           
            <div class="video" >
              <input type="hidden" name="video_val" id="video_val" value="{{news.video}}">
                <div id="myElement">Loading the player...</div>
            </div>
            <span class="clearboth"></span>
            <div class="slideshow-content">       
              <p>{{ news.description }} - {{ news.upload_date }}</p> 
            </div>
        </div>
          <!--Slideshow VIDEOS End-->
          <div class="gallery-module">
        <div class="news_title">More Videos</div>
        <div>
          {% for vid in trendingvideos %}
            <div class="gallery">
              <div class="videothumb">
                <a href="/v/{{vid.id}}">
                  <img width="100%" src="http://img1.contentapi.ws/{{vid.thumb_image}}" alt="{{vid.title}}"/>
                  <div class="video-overlay">
                    <span class="uploadedDate">{{vid.upload_date}}</span>
                    <span class="duration">{{vid.duration}}</span>
                  </div>
                  <img class="play" src="/images/play-icon.png" alt="Play"/>
                </a>
              </div>
              <a class="video-title" href="/v/{{vid.id}}">{{vid.title|slice:":35"}}</a>
            </div>
          {% endfor %}
          </div>
          </div>
      </div>       
    </aside>
    {% include "side_bar.dtl" %}
    <span class="clear"></span>
    <div class="ad1 margin20_T"> <script  src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=728X90&cwpid=552062&cwwidth=728&cwheight=90&cwpnet=1&cwtagid=178292"></script></div>
  </section>
  <!-- contentarea end --> 
</div>
<!-- container end -->
{% include "footer.dtl" %}
